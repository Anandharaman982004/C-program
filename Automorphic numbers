#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a,temp;
    scanf("%d",&a);
    temp=a;
    int p=a*a;
    if(temp>0 && temp<10)
    {
        if(p%10==temp)
        {
            printf("Automorphic Number");
        }
        else
        {
            printf("Not Automorphic Number");
        }
    }
    else
    {
        printf("Invalid Input");
    }
        
    return 0;
}
