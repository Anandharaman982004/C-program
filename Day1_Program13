#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define PI 3.14159265359
int main() {

    int a,l,br,r,ba,h;
    
    scanf("%d%d%d%d%d%d",&a,&l,&br,&r,&ba,&h);
    printf("Area of Square=%d\n",(a*a));
    printf("Area of Rectangle=%d\n",(l*br));
    
    printf("Area of Circle=%.2f\n",floor((PI*r*r)*100)/100);
     printf("Area of Triangle=%d",(int)(0.5*ba*h));
     
    return 0;
}
