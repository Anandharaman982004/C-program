#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define PI 3.14159265359
int main() {

   int a,l,w,r,b1,b2,b3;
    scanf("%d%d%d%d%d%d%d",&a,&l,&w,&r,&b1,&b2,&b3);
    printf("Perimeter of Square:%d\n",(4*a));
        printf("Perimeter of Rectangle:%d\n",(2*(l+w)));
        printf("Perimeter of Circle:%.2f\n",floor((2*r*PI)*100)/100);
        printf("Perimeter of Triangle:%d\n",(b1+b2+b3));
    
    return 0;
}
