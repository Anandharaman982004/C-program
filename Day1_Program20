#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
    int x1,y1,x2,y2;
    float a,b;
    scanf("%d\n%d\n%d\n%d",&x1,&y1,&x2,&y2);
    a=(float)(x1+x2)/2;
    b=(float)(y1+y2)/2;
    printf("Binoy's house is located at (%.1f,%.1f)",a,b);
    return 0;
}
