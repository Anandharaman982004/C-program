#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
   
      if (n<0 || n>9) 
      {
        printf("Invalid Input\n");
        return 0;
      }
    else
    {
    if(n==0)
    {
        printf("Integer is %d and String is Zero",n);
    }
        else if(n==1)
        {
            printf("Integer is %d and String is One",n);
        } 
        else if(n==2)
        {
            printf("Integer is %d and String is Two",n);
        } 
        else if(n==3)
        {
            printf("Integer is %d and String is Three",n);
        } 
        else if(n==4)
        {
            printf("Integer is %d and String is Four",n);
        } 
        else if(n==5)
        {
            printf("Integer is %d and String is Five",n);
        } 
        else if(n==6)
        {
            printf("Integer is %d and String is Six",n);
        } 
        else if(n==7)
        {
            printf("Integer is %d and String is Seven",n);
        } 
        else if(n==8)
        {
            printf("Integer is %d and String is Eight",n);
        } 
        else if(n==9)
        {
            printf("Integer is %d and String is Nine",n);
        } 
        
      
    }
        
    return 0;
}
