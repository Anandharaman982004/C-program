#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n,f=0;
    scanf("%d",&n);
    int arr[n];
    
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int ele;
    scanf("%d",&ele);
    
    for(int i=0;i<n;i++)
    {
        
    if(arr[i]==ele)
    {
        f=1;
        break;
    }
    }
    if(f){
        printf("%d is presented in an array.",ele);
      
    }
        else
        {
            printf("%d is not presented in an array.",ele);
        }
    return 0;
    
}
