#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    int a,b=1;
    scanf("%d",&a);
    int arr[a];
    for(int i=0;i<a;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int j=0;j<a;j++)
    {
        b*=arr[j];
        
    }
    printf("The Product of list value is %.2f",(float)b);
    return 0;
}
